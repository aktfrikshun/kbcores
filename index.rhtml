                    <td width="350" >
                        <% if @rid >= 3 %><%= @part.metal %><% end %>
                    </td>
                </tr>
                <tr>
                    <td   width="150"   >
                        <em>Notes&nbsp;&nbsp;&nbsp;&nbsp;</em>
                    </td>
                    <td width="350" >
                        <%= @part.notes %>
                    </td>
                    <td  width="150"   >
                        <% if @rid >= 3 %><em>KB Product Code&nbsp;&nbsp;&nbsp;&nbsp;</em><% end %>
                    </td>
                    <td width="350" >
                        <% if @rid >= 3 %><%= @part.kb_product_code %><% end %>
                    </td>
                </tr>
                <tr>
                    <td   width="150"  >
                        <% if @rid >= 3 %><em>Price&nbsp;&nbsp;&nbsp;&nbsp;</em><% end %>
                    </td>
                    <td width="350" >
                        <% if @rid >= 3 %><% if (@part.price  and (@part.price.to_f > 0.0)) %>
							<%= number_to_currency(@part.price.to_f) %>
						<% end %><% end %>
