                    <td  width="150"  >
                        <% if @rid >= 3 %><em>On Hand&nbsp;&nbsp;&nbsp;&nbsp;</em><% end %>
                    </td>
                    <td width="350" >
                        <% if @rid >= 3 %><%= @part.on_hand %><% end %>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>

